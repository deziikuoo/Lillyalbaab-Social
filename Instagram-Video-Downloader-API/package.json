{
  "name": "instagram-video-downloader-api",
  "version": "1.0.2",
  "description": "A simple Node.js API to download Instagram videos using Express and Cheerio.",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "start:ig": "node index.js",
    "start:snap": "cd ..\\Snapchat-Service && venv\\Scripts\\python.exe -m uvicorn server.main:app --reload --port 8000",
    "start:snap-frontend": "cd client/src/snapchat-frontend && npm run dev",
    "install:snap-frontend": "cd client/src/snapchat-frontend && npm install",
    "start:both": "concurrently -k -n \"Instagram,Snapchat\" -c \"cyan,magenta\" \"npm run start:ig --silent\" \"npm run start:snap --silent\"",
    "start:frontend": "cd client && npm run dev",
    "start:all": "concurrently -k -n \"Instagram,Snapchat,SnapFrontend,Frontend\" -c \"cyan,magenta,blue,green\" \"npm run start:ig --silent\" \"npm run start:snap --silent\" \"npm run start:snap-frontend --silent\" \"npm run start:frontend --silent\"",
    "monitor": "node ../monitor-services.js",
    "start:monitored": "concurrently -k -n \"API,HealthMonitor\" -c \"cyan,yellow\" \"npm start\" \"npm run monitor-health\"",
    "pm2:start": "pm2 start ../ecosystem.config.js",
    "pm2:stop": "pm2 stop ecosystem",
    "pm2:restart": "pm2 restart ecosystem",
    "pm2:logs": "pm2 logs",
    "pm2:status": "pm2 status",
    "keep-alive": "node keep-alive.js",
    "keep-alive:render": "HEALTH_URL=https://tyla-social.onrender.com/health node keep-alive.js",
    "monitor-health": "node monitor-health.js",
    "test-cache": "node test-cache.js",
    "postinstall": "npx puppeteer browsers install chrome && npm run install:snap-frontend",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "instagram",
    "video",
    "downloader",
    "api",
    "express",
    "cheerio"
  ],
  "author": "Milan Bhandari",
  "dependencies": {
    "axios": "^1.6.7",
    "cheerio": "^1.1.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "form-data": "^4.0.4",
    "mongodb": "^6.18.0",
    "node-cron": "^4.2.1",
    "node-fetch": "^3.2.6",
    "puppeteer": "^24.16.1",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
