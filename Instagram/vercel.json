{
  "version": 2,
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "buildCommand": "cd client && npm run build",
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/instagram",
      "dest": "/index.html"
    },
    {
      "src": "/snapchat",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
