{
  "version": 2,
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/snapchat-(.*)",
      "dest": "index.js"
    },
    {
      "src": "/download",
      "dest": "index.js"
    },
    {
      "src": "/gallery/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/progress/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/send-to-telegram",
      "dest": "index.js"
    },
    {
      "src": "/clear-cache",
      "dest": "index.js"
    },
    {
      "src": "/(.*)",
      "dest": "client/$1"
    }
  ]
}
