{
  "version": 2,
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/snapchat-(.*)",
      "dest": "index.js"
    },
    {
      "src": "/download",
      "dest": "index.js"
    },
    {
      "src": "/gallery/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/progress/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/send-to-telegram",
      "dest": "index.js"
    },
    {
      "src": "/clear-cache",
      "dest": "index.js"
    },
    {
      "src": "/igdl",
      "dest": "index.js"
    },
    {
      "src": "/target",
      "dest": "index.js"
    },
    {
      "src": "/start-polling",
      "dest": "index.js"
    },
    {
      "src": "/stop-polling",
      "dest": "index.js"
    },
    {
      "src": "/status",
      "dest": "index.js"
    },
    {
      "src": "/stats",
      "dest": "index.js"
    },
    {
      "src": "/set-target",
      "dest": "index.js"
    },
    {
      "src": "/poll-now",
      "dest": "index.js"
    },
    {
      "src": "/assets/(.*)",
      "dest": "client/$1"
    },
    {
      "src": "/instagram",
      "dest": "client/index.html"
    },
    {
      "src": "/snapchat",
      "dest": "client/index.html"
    },
    {
      "src": "/",
      "dest": "client/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "client/$1"
    }
  ]
}
