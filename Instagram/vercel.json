{
  "version": 2,
  "builds": [
    {
      "src": "Instagram/client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "buildCommand": "cd Instagram/client && npm install --production=false && npm run build",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
