{
  "name": "instagram",
  "version": "1.0.2",
  "description": "A simple Node.js API to download Instagram videos using Express and Cheerio.",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "build": "echo 'No build step required for Node.js backend'",
    "start:ig": "node index.js",
    "start:snap": "cd ../Snapchat-Service && .\\venv\\Scripts\\uvicorn.exe server.main:app --reload --port 8000",
    "start:frontend": "cd client && npm run dev",
    "start:both": "concurrently -k -n \"Instagram,Snapchat\" -c \"cyan,magenta\" \"npm run start:ig --silent\" \"npm run start:snap --silent\"",
    "start:all": "concurrently -k -n \"Instagram,Snapchat,Frontend\" -c \"cyan,magenta,green\" \"npm run start:ig --silent\" \"npm run start:snap --silent\" \"npm run start:frontend --silent\"",
    "monitor": "node ../monitor-services.js",
    "start:monitored": "concurrently -k -n \"API,HealthMonitor\" -c \"cyan,yellow\" \"npm start\" \"npm run monitor-health\"",
    "pm2:start": "pm2 start ../ecosystem.config.js",
    "pm2:stop": "pm2 stop ecosystem",
    "pm2:restart": "pm2 restart ecosystem",
    "pm2:logs": "pm2 logs",
    "pm2:status": "pm2 status",
    "keep-alive": "node keep-alive.js",
    "keep-alive:render": "HEALTH_URL=https://tyla-social.onrender.com/health node keep-alive.js",
    "monitor-health": "node monitor-health.js",
    "test-cache": "node test-cache.js",
    "postinstall": "mkdir -p /opt/render/project/src/data/chrome && PUPPETEER_CACHE_DIR=/opt/render/project/src/data/chrome npx puppeteer browsers install chrome",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "instagram",
    "video",
    "downloader",
    "api",
    "express",
    "cheerio"
  ],
  "author": "Milan Bhandari",
  "dependencies": {
    "@supabase/supabase-js": "^2.56.0",
    "axios": "^1.6.7",
    "cheerio": "^1.1.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "ffmpeg-static": "^5.2.0",
    "fluent-ffmpeg": "^2.1.3",
    "form-data": "^4.0.4",
    "node-cron": "^4.2.1",
    "node-fetch": "^2.7.0",
    "puppeteer": "^24.16.1",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
